{
  "name": "nedb-async",
  "version": "0.1.1",
  "description": "Promise wrapper for Nedb that makes it possible to use all Nedb all cursor modifiers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Akumzy/nedb-async"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build"
  },
  "keywords": [
    "nedb",
    "promise",
    "async",
    "await",
    "javascript",
    "database",
    "electron",
    "nw.js",
    "node"
  ],
  "author": "Akuma Isaac Akuma",
  "license": "MIT",
  "dependencies": {
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "@types/nedb": "^1.8.8",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  }
}
